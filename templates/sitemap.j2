<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in pages %}
  <url>
    <loc>{{ config.get('site_url') }}/{{ page }}</loc>
    <lastmod>{{  now.strftime('%Y-%m-%dT%H:%M%z')[:-2] }}:00</lastmod>
    <changefreq>hourly</changefreq>
  </url>
  {% endfor %}
</urlset>